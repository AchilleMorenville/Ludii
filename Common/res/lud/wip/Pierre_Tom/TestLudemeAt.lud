(game "TestLudemeAt" 
    (players 1) 
    (equipment { 
        (board (square 2) (values Cell (range 0 2)))
        (regions "Test" {0 1})
        (regions "Second" {2 3})
})


     
    (rules
        (play (satisfy {(at Most "Test" 1)
        				(is Sum "Second" 2)
